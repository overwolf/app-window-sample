{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"Sample App",
		"version":"1.0.0",
		"minimum-overwolf-version":"0.77.10",
		"author":"Overwolf",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"description":"Overwolf sample app - Creating an app window"
	},
	"data": {
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
					"file":"Files/index.html",
					"transparent":true,
					"resizable":false,
					"size":{"width":480, "height":380},
					"min_size":{"width":480, "height":380},
					"max_size":{"width":480, "height":380}
				},
		}
	}
}